<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.makancompany.project44">
    <queries>
        <package android:name="com.shopee.id" />
        <package android:name="com.spotify.music" />
        <package android:name="com.google.android.apps.calendar" />
        <package android:name="com.google.android.apps.photos" />
        <package android:name="com.android.contacts" />
        <package android:name="com.android.stk" />
        <package android:name="com.google.android.apps.maps" />
        <package android:name="com.google.android.calendar" />
    </queries>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project44">
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>